@model WebsiteViewModel

<!DOCTYPE html>
<html lang="pt-BR">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->

    <title>@Model?.Website?.TituloHeader</title>
    <link rel="shortcut icon" type="image/png" href="~/mauricio_jr/img/icone_mauricio_junior.png" />
    <link rel="shortcut icon" type="image/png" href="~/mauricio_jr/img/icone_mauricio_junior.png" />
    <!-- Google font -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700%7CVarela+Round" rel="stylesheet">

    <!-- Bootstrap -->
    <link type="text/css" rel="stylesheet" href="~/mauricio_jr/css/bootstrap.min.css" />

    <!-- Owl Carousel -->
    <link type="text/css" rel="stylesheet" href="~/mauricio_jr/css/owl.carousel.css" />
    <link type="text/css" rel="stylesheet" href="~/mauricio_jr/css/owl.theme.default.css" />

    <!-- Magnific Popup -->
    <link type="text/css" rel="stylesheet" href="~/mauricio_jr/css/magnific-popup.css" />

    <!-- Font Awesome Icon -->
    <link rel="stylesheet" href="~/mauricio_jr/css/font-awesome.min.css">

    <!-- Custom stlylesheet -->
    <link type="text/css" rel="stylesheet" href="~/mauricio_jr/css/style.css" />
    <link type="text/css" rel="stylesheet" href="~/mauricio_jr/css/style_custom.css" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css" integrity="sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/" crossorigin="anonymous">
    <link type="text/css" rel="stylesheet" href="~/css/site.css" />
</head>

<body>
    <!-- Header -->
    <header id="home">
        <!-- Background Image -->
        @{
            if (Model?.CMShome?.SlideImagem != null)
            {
                var imgBackground_base64 = Convert.ToBase64String(Model?.CMShome?.SlideImagem);
                var imgSrc_imgBackground = String.Format("data:image/gif;base64,{0}", imgBackground_base64);
                <div class="bg-img" style="background-image: url('@imgSrc_imgBackground');">
                    <div class="overlay"></div>
                </div>
            }
            else
            {
                <div class="bg-img" style="background-image: url('../../wwwroot/mauricio_jr/img/backgrounnd_home.jpg');">
                    <div class="overlay"></div>
                </div>
            }

        }

        <!-- /Background Image -->
        <!-- Nav -->
        <nav id="nav" class="navbar nav-transparent">
            <div class="container">

                <div class="navbar-header">
                    <!-- Logo -->
                    <div class="navbar-brand">
                        <a asp-action="Index">
                            @{
                                if (Model?.Website?.LogoTopo != null)
                                {
                                    var imgLogotopo_64 = Convert.ToBase64String(Model?.Website?.LogoTopo);
                                    var imgLogotopo_src = String.Format("data:image/gif;base64,{0}", imgLogotopo_64);
                                    <img class="logo" src="@imgLogotopo_src" alt="logo">
                                }
                                else
                                {
                                    <img class="logo" src="~/mauricio_jr/img/logo.png" alt="logo">
                                }
                            }
                        </a>
                    </div>
                    <!-- /Logo -->
                    <!-- Collapse nav button -->
                    <div class="nav-collapse">
                        <span></span>
                    </div>
                    <!-- /Collapse nav button -->
                </div>

                <!--  Main navigation  -->
                <ul class="main-nav nav navbar-nav navbar-right">
                    @if (Model?.AreaWebsite?.Where(x => x.IdHtml == "#home")?.FirstOrDefault()?.Ativo == true)
                    {
                        <li><a href="#home">Home</a></li>
                    }
                    @if (Model?.AreaWebsite?.Where(x => x.IdHtml == "#about")?.FirstOrDefault()?.Ativo == true)
                    {
                        <li><a href="#about">Sobre</a></li>
                    }
                    @if (Model?.AreaWebsite?.Where(x => x.IdHtml == "#service")?.FirstOrDefault()?.Ativo == true)
                    {
                        <li><a href="#service">Serviços</a></li>
                    }
                    @if (Model?.AreaWebsite?.Where(x => x.IdHtml == "#pricing")?.FirstOrDefault()?.Ativo == true)
                    {
                        <li><a href="#pricing">Planos</a></li>
                    }
                    @if (Model?.AreaWebsite?.Where(x => x.IdHtml == "#team")?.FirstOrDefault()?.Ativo == true)
                    {
                        <li><a href="#team">Parceiros</a></li>
                    }
                    @if (Model?.AreaWebsite?.Where(x => x.IdHtml == "#portfolio")?.FirstOrDefault()?.Ativo == true)
                    {
                        <li><a href="#portfolio">Galeria</a></li>
                    }
                    @if (Model?.AreaWebsite?.Where(x => x.IdHtml == "#contact")?.FirstOrDefault()?.Ativo == true)
                    {
                        <li><a href="#contact">Contato</a></li>
                    }
                </ul>
                <!-- /Main navigation -->
            </div>
        </nav>
        <!-- /Nav -->
        <!-- home wrapper -->
        <div class="home-wrapper">
            <div class="container">
                <div class="row">
                    <!-- home content -->
                    <div class="col-md-10 col-md-offset-1">
                        <div class="home-content">
                            <h2 class="white-text">@Model?.CMShome?.Titulo</h2>
                            <p class="white-text">@Model?.CMShome?.SubTitulo</p>
                            @{
                                if (Model?.CMShome?.HabilitarWhatsApp == true)
                                {
                                    <a href="https://api.whatsapp.com/send?phone=@Model?.Website?.WhatsAppNumero&text=Contato%20via%20website%3A" target="_blank"><i class="fab fa-whatsapp icone-whatsApp icone-whatsApp-contato" aria-hidden="true"></i></a>
                                }
                                if (Model?.CMShome?.HabilitarTwitter == true)
                                {
                                    <a href="@Model?.Website?.TwitterURL" target="_blank"><i class="fab fa-twitter icone-topo" aria-hidden="true"></i></a>
                                }
                                if (Model?.CMShome?.HabilitarPinterest == true)
                                {
                                    <a href="@Model?.Website?.PinterestURL" target="_blank"><i class="fab fa-pinterest icone-topo" aria-hidden="true"></i></a>
                                }
                                if (Model?.CMShome?.HabilitarInstagram == true)
                                {
                                    <a href="@Model?.Website?.InstagramURL" target="_blank"><i class="fab fa-instagram icone-instragram icone-topo" aria-hidden="true"></i></a>
                                }
                                if (Model?.CMShome?.HabilitarGoogleMais == true)
                                {
                                    <a href="@Model?.Website?.GoogleMaisURL" target="_blank"><i class="fab fa-google-plus-g icone-topo" aria-hidden="true"></i></a>
                                }
                                if (Model?.CMShome?.HabilitarFacebook == true)
                                {
                                    <a href="@Model?.Website?.FacebookURL" target="_blank"><i class="fab fa-facebook-f icone-facebook icone-topo" aria-hidden="true"></i></a>
                                }
                            }
                        </div>
                    </div>
                    <!-- /home content -->
                </div>
            </div>
        </div>
        <!-- /home wrapper -->

    </header>
    <!-- /Header -->
    <!-- About -->
    @if (Model?.AreaWebsite?.Where(x => x.IdHtml == "#about")?.FirstOrDefault()?.Ativo == true)
    {
        <div id="about" class="section md-padding">
            <!-- Container -->
            <div class="container">
                <!-- Row -->
                <div class="row">

                    <!-- Section header -->
                    <!-- <div class="section-header text-center">
                        <h2 class="title">Maurício Jr.</h2>
                    </div> -->
                    <!-- /Section header -->
                    <!-- about -->
                    <div class="col-md-6">
                        <div class="col-md-12">
                            <h3 class="title">@Model?.CMSSobre?.Titulo</h3>
                            <span>@Html.Raw(Model?.CMSSobre?.Conteudo)</span>
                        </div>
                        <div class="col-md-12">
                            <ul class="footer-follow li-redes-sociais">
                                @{
                                    if (Model?.CMSSobre?.HabilitarWhatsApp == true)
                                    {
                                        <li><a href="https://api.whatsapp.com/send?phone=@Model?.Website?.WhatsAppNumero&text=Contato%20via%20website%3A" target="_blank"><i class="fab fa-whatsapp icone-whatsApp icone-whatsApp-contato" aria-hidden="true"></i></a></li>
                                    }
                                    if (Model?.CMSSobre?.HabilitarPinterest == true)
                                    {
                                        <li><a href="@Model?.Website?.PinterestURL" target="_blank"><i class="fab fa-pinterest" aria-hidden="true"></i></a></li>
                                    }
                                    if (Model?.CMSSobre?.HabilitarInstagram == true)
                                    {
                                        <li><a href="@Model?.Website?.InstagramURL" target="_blank"><i class="fab fa-instagram icone-instragram" aria-hidden="true"></i></a></li>
                                    }
                                    if (Model?.CMSSobre?.HabilitarGoogleMais == true)
                                    {
                                        <li><a href="@Model?.Website?.GoogleMaisURL" target="_blank"><i class="fab fa-google-plus-g" aria-hidden="true"></i></a></li>
                                    }
                                    if (Model?.CMSSobre?.HabilitarFacebook == true)
                                    {
                                        <li><a href="@Model?.Website?.FacebookURL" target="_blank"><i class="fab fa-facebook-f icone-facebook" aria-hidden="true"></i></a></li>
                                    }
                                }
                            </ul>
                        </div>
                    </div>
                    <!-- /about -->
                    <!-- about -->
                    <div class="col-md-6">
                        <div class="">
                            @{
                                if (Model?.CMSSobre?.Imagem != null)
                                {
                                    var imgSobre_64 = Convert.ToBase64String(Model?.CMSSobre?.Imagem);
                                    var imgSbre_src = String.Format("data:image/gif;base64,{0}", imgSobre_64);
                                    <img src="@imgSbre_src" class="img-thumbnail" alt="Cinque Terre" style="width:897px;height:364px;">
                                }
                                else
                                {
                                    <img src="~/mauricio_jr/img/sobre_nos.jpg" class="img-thumbnail" alt="Cinque Terre">
                                }
                            }
                        </div>
                    </div>
                    <!-- /about -->

                </div>
                <!-- /Row -->

            </div>
            <!-- /Container -->

        </div>
    }
    <!-- /About -->
    <!-- Service -->
    @if (Model?.AreaWebsite?.Where(x => x.IdHtml == "#service")?.FirstOrDefault()?.Ativo == true)
    {
        <div id="service" class="section md-padding">
            <!-- Container -->
            <div class="container">
                <!-- Row -->
                <div class="row">
                    <!-- Section header -->
                    <div class="section-header text-center">
                        <h2 class="title">@Model?.CMSServicos?.Titulo</h2>
                    </div>
                    <!-- /Section header -->
                    <div class="col-md-6">
                        <div class="section-header">
                            @if (Model?.CMSServicos?.SubTitulo == true)
                            {
                                <h2 class="title">@Model?.CMSServicos?.SubtTitulo</h2>
                            }
                        </div>
                        <br>
                        <span>
                            @Html.Raw(Model?.CMSServicos?.Conteudo)
                        </span>
                    </div>
                    <!-- /why choose us content -->
                    <!-- About slider -->
                    <div class="col-md-6">
                        <div id="about-slider" class="owl-carousel owl-theme">
                            @{
                                if (Model?.CMSServicos?.Imagem != null)
                                {
                                    var imgServicos_64 = Convert.ToBase64String(Model?.CMSServicos?.Imagem);
                                    var imgSbre_src = String.Format("data:image/gif;base64,{0}", imgServicos_64);
                                    <img class="img-responsive" src="@imgSbre_src" style="width:640px;height:426px" alt="">
                                }
                                else
                                {
                                    <img class="img-responsive" src="~/mauricio_jr/img/sport-2264825_640.jpg" alt="">
                                }
                            }
                        </div>
                    </div>


                </div>
                <!-- /Row -->
            </div>
            <!-- /Container -->
        </div>
    }

    <!-- Numbers -->
    <div id="numbers" class="section sm-padding">
        <!-- Background Image -->
        @if (Model?.CMSServicos?.ImagemCitacao != null)
        {
            var imgServicosCitacao_64 = Convert.ToBase64String(Model?.CMSServicos?.ImagemCitacao);
            var imgServicosCitacao_src = String.Format("data:image/gif;base64,{0}", imgServicosCitacao_64);
            <div class="bg-img" style="background-image: url('@imgServicosCitacao_src');">
                <div class="overlay"></div>
            </div>
        }
        else
        {
            <div class="bg-img" style="background-image: url('~/mauricio_jr/img/background2.jpg');">
                <div class="overlay"></div>
            </div>
        }

        <!-- /Background Image -->
        <!-- Container -->
        <div class="container">
            <!-- Row -->
            <div class="row">
                <h3 class="texto-frase">
                    “@Model?.CMSServicos?.TextoCitacao”
                </h3>
            </div>
            <!-- /Row -->
        </div>
        <!-- /Container -->
    </div>
    <!-- /Numbers -->
    <!-- Pricing -->
    @if (Model?.AreaWebsite?.Where(x => x.IdHtml == "#pricing")?.FirstOrDefault()?.Ativo == true)
    {
        <div id="pricing" class="section md-padding">
            <!-- Container -->
            <div class="container">
                <!-- Row -->
                <div class="row">
                    <!-- Section header -->
                    <div class="section-header text-center">
                        <h2 class="title">@Model?.CMSPlanos?.Titulo</h2>
                    </div>
                    <!-- /Section header -->
                    <!-- pricing -->
                    @if (Model?.CMSTabelaDePlanos != null)
                    {
                        foreach (var item in Model?.CMSTabelaDePlanos)
                        {
                            <div class="col-sm-4">
                                <div class="pricing">
                                    <div class="price-head">
                                        <span class="price-title">@item.TextoHeader</span>
                                        <div class="price">
                                            <h3>R$@item.ValorPlano<span class="duration"></span></h3>
                                        </div>
                                    </div>
                                    <ul class="price-content">
                                        <li>
                                            <p>@item.TextoFooter</p>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        }
                    }
                    <!-- /pricing -->
                </div>
                <!-- Row -->
            </div>
            <!-- /Container -->
        </div>
    }

    <!-- /Team -->
    @if (Model?.AreaWebsite?.Where(x => x.IdHtml == "#team")?.FirstOrDefault()?.Ativo == true)
    {
        <div id="team" class="section md-padding">
            <!-- Container -->
            <div class="container container_equipe">
                <!-- Row -->
                <div class="row">
                    <!-- Section header -->
                    <div class="section-header text-center">
                        <h2 class="title">Nossos Parceiros</h2>
                    </div>
                    <!-- /Section header -->
                    <!-- team -->
                    @{
                        if (Model?.CardParceiros != null)
                        {
                            foreach (var item in Model?.CardParceiros)
                            {
                                var imgParceiro_64 = Convert.ToBase64String(item?.LogoParceiro);
                                var imgParceiro_src = String.Format("data:image/gif;base64,{0}", imgParceiro_64);
                                <div class="col-sm-4 ">
                                    <div class="team">
                                        <div class="team-img">
                                            <img class="img-responsive" src="@imgParceiro_src" style="width:350px;height:315px;" alt="">
                                            <div class="overlay">
                                                <div class="team-social">
                                                    @{
                                                        if (item.HabilitarFacebook == true)
                                                        {
                                                            <a href="@item.FacebookURL" target="_blank"><i class="fab fa-facebook-f"></i></a>
                                                        }
                                                        if (item.HabilitarGoogleMais == true)
                                                        {
                                                            <a href="@item.GoogleMaisURL" target="_blank"><i class="fab fa-chrome"></i></a>
                                                        }
                                                        if (item.HabilitarInstagram == true)
                                                        {
                                                            <a href="@item.InstagramURL" target="_blank"><i class="fab fa-instagram"></i></a>
                                                        }
                                                        if (item.HabilitarPinterest == true)
                                                        {
                                                            <a href="@item.PinterestURL" target="_blank"><i class="fab fa-pinterest"></i></a>
                                                        }
                                                    }
                                                </div>
                                            </div>
                                        </div>
                                        <div class="team-content">
                                            <h3>@item.NomeParceiro</h3>
                                            <span>@item.Descricao</span>
                                        </div>
                                    </div>
                                </div>
                            }
                        }
                    }
                </div>
                <!-- /Row -->
            </div>
            <!-- /Container -->
        </div>
    }
    <!-- /Team -->
    <!-- /Blog -->
    <!-- Galeria -->
    @if (Model?.AreaWebsite?.Where(x => x.IdHtml == "#portfolio")?.FirstOrDefault()?.Ativo == true)
    {
        <div id="portfolio" class="section md-padding bg-grey">
            <!-- Container -->
            <div class="container">
                <!-- Row -->
                <div class="row">
                    <!-- Section header -->
                    <div class="section-header text-center">
                        <h2 class="title">Galeria de Fotos</h2>
                    </div>
                    <!-- /Section header -->
                    @{
                        if (Model?.ImagemGaleria != null)
                        {
                            foreach (var item in Model?.ImagemGaleria)
                            {
                                var imgGaleria_64 = Convert.ToBase64String(item?.Imagem);
                                var imgGaleria_src = String.Format("data:image/gif;base64,{0}", imgGaleria_64);
                                <div class="col-md-4 col-xs-6 work">
                                    <img class="img-responsive" src="@imgGaleria_src" style="width:400px;height:400px;" alt="">
                                    <div class="overlay"></div>
                                    <div class="work-content">
                                        <!-- <span>Category</span> -->
                                        <!-- <h3>Lorem ipsum dolor</h3> -->
                                        <div class="work-link">
                                            <a href="#"><i class="fas fa-external-link-alt"></i></a>
                                            <a class="lightbox" href="@imgGaleria_src"><i class="fas fa-search"></i></a>
                                        </div>
                                    </div>
                                </div>
                            }
                        }
                    }
                </div>

            </div>
            <!-- /Container -->

        </div>
    }
    <!-- /Portfolio -->
    <!-- Contact -->
    @if (Model?.AreaWebsite?.Where(x => x.IdHtml == "#contact")?.FirstOrDefault()?.Ativo == true)
    {
        <div id="contact" class="section md-padding">

            <!-- Container -->
            <div class="container">

                <!-- Row -->
                <div class="row">

                    <!-- Section-header -->
                    <div class="section-header text-center">
                        <h2 class="title">Fale Conosco</h2>
                    </div>
                    <!-- /Section-header -->
                    <!-- contact -->
                    @if (Model?.CMSFaleConosco?.HabilitarWhatsApp == true)
                    {
                        <div class="col-sm-4">
                            <div class="contact">
                                <i class="fab fa-whatsapp icone-whatsApp" aria-hidden="true"></i>
                                <p>@Model?.Website?.WhatsAppNumero</p>
                            </div>
                        </div>
                    }
                    <!-- /contact -->
                    <!-- contact -->
                    @if (Model?.CMSFaleConosco?.HabilitarEmail == true)
                    {
                        <div class="col-sm-4">
                            <div class="contact">
                                <i class="fa fa-envelope icone-email"></i>
                                <p>@Model?.CMSFaleConosco?.Email</p>
                            </div>
                        </div>
                    }
                    <!-- /contact -->
                    <!-- contact -->
                    @if (Model?.CMSFaleConosco?.HabilitarLocalizacao == true)
                    {
                        <div class="col-sm-4">
                            <div class="contact">
                                <i class="fa fa-map-marker icone-localizacao"></i>
                                <p>@Model?.CMSFaleConosco?.Localizacao</p>
                            </div>
                        </div>
                    }
                    <!-- /contact -->
                    <!-- contact form -->
                    <div class="col-md-8 col-md-offset-2">
                        <form class="contact-form" asp-action="EnviarEmail" name="form_contato" method="post">
                            <input type="text" class="input" placeholder="Nome" name="nome_contato" id="nome_contato">
                            <input type="tel" class="input" placeholder="Telefone" name="cel_contato" id="cel_contato">
                            <input type="email" class="input" placeholder="Email" name="email_contato" id=email_contato>
                            <input type="text" class="input" placeholder="Assunto" name="assunto_contato" id="assunto_contato">
                            <textarea class="input" placeholder="Mensagem" name="msg_contato" id="msg_contato"></textarea>
                            <button class="main-btn">Enviar Mensagem</button>
                        </form>
                    </div>
                    <!-- /contact form -->

                </div>
                <!-- /Row -->

            </div>
            <!-- /Container -->

        </div>
    }
    <!-- /Contact -->
    <!-- Footer -->
    <footer id="footer" class="sm-padding bg-dark">
        <!-- Container -->
        <div class="container">
            <!-- Row -->
            <div class="row">

                <div class="col-md-12">

                    <ul class="footer-follow">
                        @{
                            if (!String.IsNullOrEmpty(Model?.Website?.WhatsAppNumero))
                            {
                                <li><a href="https://api.whatsapp.com/send?phone=@Model?.Website?.WhatsAppNumero&text=Contato%20via%20website%3A" target="_blank"><i class="fab fa-whatsapp icone-whatsApp icone-whatsApp-contato" aria-hidden="true"></i></a></li>
                            }
                            if (!String.IsNullOrEmpty(Model?.Website?.FacebookURL))
                            {
                                <li><a href="@Model?.Website?.FacebookURL" target="_blank"><i class="fab fa-facebook-f icone-facebook" aria-hidden="true"></i></a></li>
                            }
                            if (!String.IsNullOrEmpty(Model?.Website?.GoogleMaisURL))
                            {
                                <li><a href="@Model?.Website?.GoogleMaisURL" target="_blank"><i class="fab fa-google-plus-g" aria-hidden="true"></i></a></li>
                            }
                            if (!String.IsNullOrEmpty(Model?.Website?.InstagramURL))
                            {
                                <li><a href="@Model?.Website?.InstagramURL" target="_blank"><i class="fab fa-instagram icone-instragram" aria-hidden="true"></i></a></li>
                            }
                            if (!String.IsNullOrEmpty(Model?.Website?.PinterestURL))
                            {
                                <li><a href="@Model?.Website?.PinterestURL" target="_blank"><i class="fab fa-pinterest" aria-hidden="true"></i></a></li>
                            }
                        }

                        <li style="margin-left: 30px;">
                            <div>
                                <p>Desenvolvido por <b>SOUSA SILVA ENGENHARIA DE SISTEMAS</b> </p>
                            </div>
                            @*<div>
                                <a href="http://www.gssolucoes.com.br/" target="_blank" style="width: 100px;height: 51px;">
                                    <img src="~/mauricio_jr/img/GSlogo.png" alt="logo">
                                </a>
                            </div>*@
                        </li>
                    </ul>
                    <!-- /footer follow -->
                    <!-- footer copyright -->
                    <div class="footer-copyright">
                        <p>Copyright © 2018 - @DateTime.Now.Year. Todos direitos reservado.</p>
                    </div>

                </div>

            </div>
            <!-- /Row -->
        </div>
        <!-- /Container -->

    </footer>
    <!-- /Footer -->
    <!-- Back to top -->
    <div id="whatsApp-contato">
        <a href="https://api.whatsapp.com/send?phone=@Model?.Website?.WhatsAppNumero&text=Contato%20via%20website%3A" target="_blank">
            <i class="fab fa-whatsapp icone-whatsApp icone-whatsApp-contato" aria-hidden="true"></i>
        </a>
    </div>
    <div id="back-to-top"></div>
    <!-- /Back to top -->
    <!-- Preloader -->
    <div id="preloader">
        <div class="preloader">
            <span></span>
            <span></span>
            <span></span>
            <span></span>
        </div>
    </div>
    <!-- /Preloader -->
    <!-- jQuery Plugins -->
    <script type="text/javascript" src="~/mauricio_jr/js/jquery.min.js"></script>
    <script type="text/javascript" src="~/mauricio_jr/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/mauricio_jr/js/owl.carousel.min.js"></script>
    <script type="text/javascript" src="~/mauricio_jr/js/jquery.magnific-popup.js"></script>
    <script type="text/javascript" src="~/mauricio_jr/js/main.js"></script>
    <script type="text/javascript" src="~/mauricio_jr/js/jquery.mask.min.js"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            $("#cel_contato").mask("(00) 90000-0000");
        });
    </script>
</body>

</html>